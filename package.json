{
  "name": "type-r",
  "version": "3.0.0-a06",
  "description": "Reactive serializable data layer for modern JS applications",
  "main": "./dist/index.js",
  "jsnext:main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "src",
    "endpoints",
    "ext-types",
    "globals"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build:lib": "npm run compile:lib && npm run bundle:lib",
    "compile:lib": "node_modules/.bin/tsc",
    "bundle:lib": "node_modules/.bin/rollup --config",
    "test:backbone": "node_modules/.bin/qunit 'tests/backbone-regression/*.tst.js'",
    "test:type-r": "node_modules/.bin/jest",
    "test": "npm run test:backbone && npm run test:type-r",
    "build": "npm run build:lib && npm run build:endpoints && npm run build:ext-types && npm run build:globals && npm test",
    "docs": "node docs/build.js",
    "publish:alpha": "npm publish --tag alpha",
    "publish:beta": "npm publish --tag beta",
    "build:endpoints": "cd endpoints/attributes && npm run build && cd ../localStorage && npm run build && cd ../memory && npm run build && cd ../restful && npm run build && cd ../proxy && npm run build",
    "build:ext-types": "cd ext-types && npm run build",
    "build:globals": "cd globals && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaperton/TransactionalJS.git"
  },
  "keywords": [
    "model",
    "backbone",
    "data",
    "nestedtypes",
    "schema",
    "serialization"
  ],
  "author": "Vlad Balin",
  "contributors": [
    "Ivan Terpugov <ru.ivan.alexandrovich@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaperton/TransactionalJS/issues"
  },
  "homepage": "https://github.com/gaperton/TransactionalJS#readme",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "handlebars": "^4.0.12",
    "highlight.js": "^9.13.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.6.0",
    "marked": "^0.3.19",
    "nock": "^9.6.1",
    "qunit": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rollup": "^0.66.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "*",
    "rollup-plugin-paths": "*",
    "rollup-plugin-sourcemaps": "*",
    "rollup-plugin-uglify": "^6.0.0",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.3.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.1",
    "underscore": "*"
  },
  "dependencies": {
    "tslib": "*"
  }
}
